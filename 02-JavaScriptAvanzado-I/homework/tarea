 var snack = 'Meow Mix';

 function getFood(food) {
     if (food) {
         var snack = 'Friskies';
         return snack; // Friskies
     }
     return snack; // Meow Mix
 }
 
 getFood(false);


var fullname = 'Juan Perez';
var obj = {
   fullname: 'Natalia Nerea',
   prop: {
      fullname: 'Aurelio De Rosa',
      getFullname: function() {
         return this.fullname; // Aurelio De Rosa
      }
   }
};

console.log(obj.prop.getFullname()); // Juan Perez

var test = obj.prop.getFullname; // Natalia Nerea

console.log(test()); //


function printing() {
    console.log(1); // primer
    setTimeout(function() { console.log(2); }, 1000); // cuarto
    setTimeout(function() { console.log(3); }, 0); // tercero
    console.log(4); // segundo
 }
 
 printing();